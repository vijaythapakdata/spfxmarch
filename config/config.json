{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "hello-world-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/helloWorldWebpart/HelloWorldWebpartWebPart.js",
          "manifest": "./src/webparts/helloWorldWebpart/HelloWorldWebpartWebPart.manifest.json"
        }
      ]
    },
    "sample-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sample/SampleWebPart.js",
          "manifest": "./src/webparts/sample/SampleWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "HelloWorldWebpartWebPartStrings": "lib/webparts/helloWorldWebpart/loc/{locale}.js",
    "SampleWebPartStrings": "lib/webparts/sample/loc/{locale}.js"
  }
}
